{
  "name": "graylog-web-interface",
  "version": "1.2.3-SNAPSHOT",
  "description": "Graylog Web Interface",
  "author": "torch",
  "license": "GPL",
  "scripts": {
    "build": "./node_modules/.bin/webpack",
    "test": "./node_modules/karma/bin/karma start karma.ci.conf.js",
    "prepare-dev": "./tools/prepare-dev",
    "watch": "cp templates/scripts.scala.html.dev.template ../app/views/build/scripts.scala.html && ./node_modules/.bin/webpack --watch --devtool eval --config webpack.dev.config.js",
    "hot": "cp templates/scripts.scala.html.hmr.template ../app/views/build/scripts.scala.html && ./node_modules/.bin/webpack-dev-server --hot --inline --config webpack.hmr.config.js --content-base ../public --devtool eval"
  },
  "dependencies": {
    "c3": "^0.4.11",
    "crossfilter": "1.3.x",
    "d3": "3.5.x",
    "dc": "2.0.0-beta.12",
    "immutable": "^3.7.3",
    "markdown": "^0.5.0",
    "moment": "2.10.2",
    "moment-duration-format": "1.3.0",
    "numeral": "1.5.x",
    "qs": "2.3.x",
    "react": "0.13.x",
    "react-bootstrap": "0.23.x",
    "react-zeroclipboard": "^1.1.0",
    "sockjs-client": "1.0.x"
  },
  "devDependencies": {
    "awesome-typescript-loader": "^0.9.2",
    "babel-core": "^5.8.12",
    "babel-eslint": "^4.0.5",
    "babel-loader": "^5.3.2",
    "clean-webpack-plugin": "^0.1.3",
    "eslint": "^0.24.1",
    "eslint-config-airbnb": "0.0.7",
    "eslint-loader": "^0.14.2",
    "eslint-plugin-import": "^0.7.8",
    "eslint-plugin-react": "^3.1.0",
    "html-webpack-plugin": "^1.6.0",
    "jasmine-core": "*",
    "jquery": "*",
    "json-loader": "^0.5.2",
    "karma": "^0.12.37",
    "karma-chrome-launcher": "*",
    "karma-html2js-preprocessor": "*",
    "karma-jasmine": "*",
    "karma-phantomjs-launcher": "*",
    "karma-webpack": "*",
    "react-hot-loader": "^1.2.8",
    "typescript": "^1.5.3",
    "webpack": "^1.10.5",
    "webpack-dev-server": "^1.10.1"
  }
}
